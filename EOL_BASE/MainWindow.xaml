<Window x:Class="EOL_BASE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EOL Inspector" Height="950" Width="1250" WindowStartupLocation="CenterScreen">
    <Border >
        <Border.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFFFF4F4" Offset="1"/>
                <GradientStop Color="White" Offset="0"/>
            </LinearGradientBrush>
        </Border.Background>
        <Grid Grid.ColumnSpan="2" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="160" Name="isNeedDCIRPanel"/>
                <ColumnDefinition Width="162*"/>
                <ColumnDefinition Width="707*"/>
                <ColumnDefinition Width="213" MaxWidth="250" MinWidth="10"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="210"/>
                <RowDefinition Height="90" Name="btgridHeight"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="0" Name="isNeedOCV"/>
                <RowDefinition Height="180"/>
                <RowDefinition Height="150"/>
            </Grid.RowDefinitions>
            <Border BorderBrush="Gray" BorderThickness="2" Margin="1" CornerRadius="2">

                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="8.5*"/>

                    </Grid.ColumnDefinitions>
                    <StackPanel Margin="5">
                        <Label Content="Cycler" Height="30" Margin="0,0,0,5" FontSize="16" Foreground="White" Background="Black"/>
                        <Label Content="Ready" Height="45" Margin="0,0,0,5" FontSize="16" Name="mainCStateLb1" Foreground="White" Background="Gray"/>
                        <Label Content="0.00 V" Height="50" Margin="0,0,0,5" FontSize="25" Background="Black" Foreground="White" FontWeight="Bold" HorizontalContentAlignment="Right" Padding="0,0,10,0" Name="mainVoltageLb1"/>
                        <Label Content="0.00 A" Height="50" Margin="0,0,0,5" FontSize="25" Background="Black" Foreground="White" FontWeight="Bold" HorizontalContentAlignment="Right" Padding="0,0,10,0" Name="mainCurrentLb1"/>
                    </StackPanel>
                </Grid>
            </Border>

            <Grid Grid.Column="1" Grid.Row="0" Margin="1,1,1,19" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>

                </Grid.RowDefinitions>
                <UniformGrid Grid.Row="0" Grid.ColumnSpan="4" Columns="1" >
                    <Label Content="LV Connector 0/0"       Margin="2" FontSize="11" Background="Green"     Foreground="White"  FontWeight="Bold" Name="lbLvconnector"  BorderBrush="Black" BorderThickness="2" />
                </UniformGrid>
                <UniformGrid Grid.Row="1" Grid.ColumnSpan="4" Columns="1" >
                    <Label Content="Current Probe 0/0"      Margin="2" FontSize="11" Background="Green"     Foreground="White"  FontWeight="Bold" Name="lbCurrentProbe" BorderBrush="Black" BorderThickness="2"/>
                </UniformGrid>
                <UniformGrid Grid.Row="2" Grid.ColumnSpan="4" Columns="1" >
                    <Label Content="Hipot-Pin 0/0"      Margin="2" FontSize="11" Background="Green"     Foreground="White"  FontWeight="Bold" Name="lbHipotPin" BorderBrush="Black" BorderThickness="2"/>
                </UniformGrid>
                <UniformGrid Grid.Row="3" Grid.ColumnSpan="4" Columns="1" >
                    <Label Content="MC 1 0/0"               Margin="2" FontSize="11" Background="Green"     Foreground="White"  FontWeight="Bold" Name="lbMc1"          BorderBrush="Black" BorderThickness="2"/>
                </UniformGrid>
                <UniformGrid Grid.Row="4" Grid.ColumnSpan="4" Columns="1" >
                    <Label Content="MC 2 0/0"               Margin="2" FontSize="11" Background="Green"     Foreground="White"  FontWeight="Bold" Name="lbMc2"          BorderBrush="Black" BorderThickness="2"/>
                </UniformGrid>
                <UniformGrid Grid.Row="5" Grid.ColumnSpan="4" Columns="1" >
                    <Label Content="MC 3 0/0"               Margin="2" FontSize="11" Background="Green"     Foreground="White"  FontWeight="Bold" Name="lbMc3"          BorderBrush="Black" BorderThickness="2"/>
                </UniformGrid>
                <UniformGrid Grid.Row="6" Grid.ColumnSpan="4" Columns="1" >
                    <Button Grid.Row="1" Grid.ColumnSpan="3" Content="Replace / Reset" FontSize="11" Click="btnOpenCntSetting" Name="btnDcirOpenCntSetting" />
                </UniformGrid>

            </Grid>

            <Grid Name="TopGrid" Grid.Column="2" Grid.Row="0" Margin="7,5,5,5" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.5*"/>
                    <ColumnDefinition Width="8*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Label Content="Lot ID" HorizontalAlignment="Right" Padding="0,0,5,0"/>
                <Label Content="Module Barcode" Grid.Row="1" HorizontalAlignment="Right" Padding="0,0,5,0" />
                <Label Content="Product ID" Grid.Row="2" HorizontalAlignment="Right" Padding="0,0,5,0"/>
                <Label Content="Process ID" Grid.Row="3" HorizontalAlignment="Right" Padding="0,0,5,0"/>
                <Label Content="Equip ID" Grid.Row="4" HorizontalAlignment="Right" Padding="0,0,5,0"/>
                <Label Content="User ID" Grid.Row="5" HorizontalAlignment="Right" Padding="0,0,5,0"/>

                <Label Content="Start Time" Grid.Column="0" Grid.Row="6" HorizontalAlignment="Right" Padding="0,0,5,0"/>
                <Label Content="Finish Time" Grid.Column="0" Grid.Row="7" HorizontalAlignment="Right" Padding="0,0,5,0"/>
                <Label Content="Elapsed Time" Grid.Column="0" Grid.Row="8" HorizontalAlignment="Right" Padding="0,0,5,0"/>


                <TextBox Grid.Column="1" Background="White"  VerticalAlignment="Center" Name="lotTb" FontSize="12" Text="test_Barcode" IsReadOnly="False" HorizontalContentAlignment="Center" FontWeight="Bold"/>

                <TextBox Grid.Column="1" Grid.Row="1" Background="White"  VerticalAlignment="Center" Name="monoTb" FontSize="12" Text="TEMP_MONOFRAME" IsReadOnly="False" HorizontalContentAlignment="Center" FontWeight="Bold"/>

                <TextBox Grid.Column="1" FontSize="12" Grid.Row="2"   HorizontalContentAlignment="Center" Text="DEV" Name="prodTb" IsReadOnly="True" FontWeight="SemiBold"/>
                <TextBox Grid.Column="1" FontSize="12" Grid.Row="3"   HorizontalContentAlignment="Center" Text="DEV" Name="procTb" FontWeight="SemiBold" IsReadOnly="True"/>
                <TextBox Grid.Column="1" FontSize="12" Grid.Row="4"   HorizontalContentAlignment="Center" Text="DEV" Name="equipTb" FontWeight="SemiBold" IsReadOnly="True"/>
                <TextBox Grid.Column="1" FontSize="12" Grid.Row="5"   HorizontalContentAlignment="Center" Text="DEV" Name="userTb"  IsReadOnly="True"/>

                <TextBox   FontWeight="SemiBold" Margin="0,1" FontSize="12" Grid.Row="6" Grid.Column="1" HorizontalContentAlignment="Center"  Name="time_start_tb" IsHitTestVisible="False" Background="#FFECECEC"/>

                <TextBox  FontWeight="SemiBold" Margin="0,1" Grid.Column="1" Grid.Row="7" FontSize="12" IsReadOnly="True" HorizontalContentAlignment="Center" Name="time_finish_tb" IsHitTestVisible="False" Background="#FFECECEC"/>

                <TextBox  FontWeight="SemiBold" Margin="0,1" FontSize="12" Grid.Column="1" Grid.Row="8"  IsReadOnly="True" HorizontalContentAlignment="Center" Name="time_elapsed_tb" IsHitTestVisible="False" Background="#FFECECEC"/>

            </Grid>

            <Grid Grid.Row="1" Grid.ColumnSpan="3" Margin="5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <UniformGrid Grid.Row="1" Grid.ColumnSpan="1" Rows="2" Margin="2" Name="btgrid">

                </UniformGrid>


                <Grid Grid.Row="1" Grid.ColumnSpan="3" Background="DarkGray" Opacity="0.6" Visibility="Visible" Name="blinder" Margin="3" />
            </Grid>

            <DataGrid AutoGenerateColumns="False" FontWeight="SemiBold" FontSize="13" Name="testItemListDg" VerticalContentAlignment="Center"  SelectionMode="Extended" 
                      CanUserAddRows="False" Grid.ColumnSpan="3"  Grid.Row="2" MouseDoubleClick="testItemListDg_MouseDoubleClick" ItemsSource="{Binding TestItemList}" Margin="5" >
                <DataGrid.Columns>
                    <DataGridTextColumn FontWeight="SemiBold"  Header="No" Binding="{Binding Path=Value.No}" Width="45" IsReadOnly="True" CanUserSort="False" />
                    <DataGridTextColumn FontWeight="SemiBold" Header="CLCTITEM" Binding="{Binding Path=Value.CLCTITEM}" Width="10" IsReadOnly="True" CanUserSort="False"/>

                    <DataGridTextColumn FontWeight="SemiBold" Header="Name" Binding="{Binding Path=Key}" Width="5*" IsReadOnly="True" CanUserSort="False"/>
                    <DataGridTextColumn FontWeight="SemiBold" Header="Min" Binding="{Binding Path=Value.Min}" Width="1*" IsReadOnly="True" CanUserSort="False"/>
                    <DataGridTextColumn FontWeight="SemiBold" Header="Value" Binding="{Binding Path=Value.Value_}" Width="1.5*" IsReadOnly="True" CanUserSort="False"/>
                    <DataGridTextColumn FontSize="13" FontWeight="Normal" Header="Unit" Binding="{Binding Path=Value.Unit}" Width="0.7*" IsReadOnly="True" CanUserSort="False"/>
                    <DataGridTextColumn FontWeight="SemiBold" Header="Max" Binding="{Binding Path=Value.Max}" Width="1*" IsReadOnly="True" CanUserSort="False"/>
                    <DataGridTextColumn FontWeight="SemiBold" Header="Result" Binding="{Binding Path=Value.Result}" Width="1.5*" IsReadOnly="True" CanUserSort="False" />
                </DataGrid.Columns>
            </DataGrid>

            <!--2018.11.09 jeonhj CMA 검사기에서 Cell 전압을 읽어 보여주는 화면 추가-->
            <Grid Grid.Row="3" Grid.ColumnSpan="3" Margin="1,1,1,179" Grid.RowSpan="2">
                <DataGrid LayoutUpdated="CellDetailDg_LayoutUpdated_1" AutoGenerateColumns="False" FontWeight="SemiBold" FontSize="10" Name="OCVDetailDg" VerticalContentAlignment="Center"  SelectionMode="Extended" CanUserAddRows="False"  >
