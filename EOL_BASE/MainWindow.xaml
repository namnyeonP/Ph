                        <DataGridTextColumn FontWeight="SemiBold" Header="Cell Voltage" Binding="{Binding Path=TestName}" Width="3*" IsReadOnly="True" CanUserSort="False" />
                        <DataGridTextColumn FontWeight="SemiBold" Header="1" Binding="{Binding Path=CellVolt_1}" Width="0.8*" IsReadOnly="True" CanUserSort="False"/>
