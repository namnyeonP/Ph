<Window x:Class="EOL_BASE.윈도우.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EOL_BASE.윈도우"
        mc:Ignorable="d"
        Title="ProgressWindow" Height="40" Width="300" WindowStartupLocation="CenterScreen" WindowStyle="None">
    <Border BorderThickness="1">
    <Grid>
        <Grid.RowDefinitions>
                <RowDefinition Height="0"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <ListBox Name="lb" Margin="1" Background="DarkGray" Foreground="White">
            
        </ListBox>
        <ProgressBar Grid.Row="1" Name="pb" Margin="1" IsIndeterminate="True"/>
    </Grid>
    </Border>
</Window>
